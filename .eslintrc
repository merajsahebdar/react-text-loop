{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [
          "type",
          "builtin",
          "external",
          "internal",
          ["parent", "index", "sibling"],
          "object"
        ],
        "warnOnUnassignedImports": true,
        "newlines-between": "never",
        "alphabetize": { "order": "asc", "caseInsensitive": true }
      }
    ]
  }
}
